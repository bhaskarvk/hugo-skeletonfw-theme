@import url('https://fonts.googleapis.com/css?family=Raleway:300,300i,400,400i,700,700i')
@import url('https://fonts.googleapis.com/css?family=Josefin+Slab:300,300i,400,400i,700,700i')
@import url('https://fonts.googleapis.com/css?family=Source+Code+Pro:300,300i,400,400i,700,700i')
body
  font-family: "Raleway", sans-serif

h1,
h2,
h3,
h4,
h5,
h6
  font-family: "Josefin Slab", sans-serif
  letter-spacing: 0.2px
  font-weight: bold

pre,
code
  font-family: 'Source Code Pro', monospace

